.tabbar_box{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 128rpx;
    box-shadow: 0 0 2px rgba(0, 0, 0, 0.1);
}
.tabbar_box.iphoneX-height{
    padding-bottom:30rpx;
}
.middle-wrapper{
  position: absolute;
  right: 310rpx;
  bottom: 0;
  background-color: #fff;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border-top: 2rpx solid #313199;
}
.middle-wrapper.iphoneX-height{
  bottom: 66rpx;
}
.tabbar_nav{
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    height: 100%;
    position: relative;
}
.tabbar_icon{
    width: 56rpx;
    height: 56rpx;
}
.special-wrapper{
  position: absolute;
  left: 36rpx;
  top: -36rpx;
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
  border-top: 1rpx solid #f2f2f3;
  background-color: #f2f2f3;
  text-align: center;
  box-sizing: border-box;
  padding: 0rpx;
}
.special-wrapper .tabbar_icon{
    width: 83rpx;
    height: 83rpx;
}
.special-text-wrapper{
  width: 42rpx;
  height: 42rpx;
}
/*个人信息样式*/

.user{
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  margin:2rpx 0 2rpx 0;
  /* background-color: #F2F2F2; */
  width: 100%;
  height: 212rpx;
}

.head image{
  width: 160rpx;
  height: 160rpx;
  margin: 26rpx 24rpx 26rpx 34rpx;
}
.details{
  display: flex;
  flex-wrap: wrap;
  flex-direction: column;
  margin-top: 56rpx;
}
.details .name{
  font-size: 36rpx;
  color: #000000;
}
.details .byear{
  color:#6852fd;
  font-size: 28rpx;
  margin: 82rpx 0 0 -20rpx;
}
.details .gyear{
  color:#ee5e7b;
  font-size: 28rpx;
  margin: 82rpx 0 0 -20rpx;
}
.details image{

  margin-top: 24rpx;
  display: flex;
  /* flex-wrap: wrap;
  flex-direction: column; */
}
.type{
    margin: 56rpx 0 0 40rpx;
}
.type .typec{
  color: #6852fd;
  font-size: 26rpx;
  border: 3rpx #6852fd solid;
  border-radius: 10rpx;
  margin: 0 5rpx 0 5rpx;
  
}
.type .taken{
  margin-top: 40rpx;
}
.type .taken .nums{
  color:#999999;
  font-size: 28rpx;
}
.type image{
  width: 10rpx;
  height: 20rpx;
  margin-left: 10rpx;
}
.details image{
  width: 80rpx;
  height: 40rpx;
  z-index: 88;
}
.grades{
  display: flex;
  flex-wrap: wrap;
  flex-direction: column;
  padding: 110rpx 0rpx 0rpx 100rpx;
}
.grades text{
  font-size: 50rpx;
  color: #6852fd;
}



/*tab框样式*/

.flex-row{
  flex-direction: row;
  display: flex;
}
.onTabLine{
  width: 120rpx;
  height: 5rpx;
  background-color:#4074DD;
  margin-top:15rpx;
}
.line{
  width: 100%;
  height: 2rpx;
  background-color: #F2F2F2;
  margin: 0rpx auto 0 auto;
}
.Photographer,.place,.service{
  font-size:36rpx;
  margin-top: 20rpx;
  width: 200rpx;
  color:#999999;
}
.synthesize,.feedback,.deal,.screen{
  font-size: 34rpx;
  margin-top: 20rpx;
  width: 170rpx;
  color: #999999;
}

.Photographer{
  margin-left: 60rpx;
}
.place{
  margin-left: 29rpx;
}
.service{
  margin-left: 50rpx;
}
.synthesize{
  margin-left: 56rpx;
}
.feedback{
  margin-left: 20rpx;
}
.deal{
  margin-left: 8rpx;
}
.screen{
  margin-left: 12rpx;
}
.active{
  color:#4074DD;
}
.selectTab{
  position: fixed;
  z-index: 999;
  color: #ffffff;
}
.selectbody{
  position: fixed;
  z-index: 998;
  height: 80rpx;
  background-color: #ffffff;
}
.synthesize image{
  width: 20rpx;
  height: 10rpx;
  margin-bottom: 10rpx;
} 
/*城市选择框样式*/ 
.location{
  font-size: 36rpx;
  font-weight: 550;
  color: #000000;
  margin-left: 40rpx;
  margin-top: 10rpx;
}
.location image{
  width: 25rpx;
  height: 20rpx;
  margin-bottom: 8rpx;
}
/*搜索框样式*/ 

.navBar{
  margin-top:4px;
  position: fixed;
  height:40px;
  z-index: 9999;
}
.searchfield{
  flex-direction: row;
  display: flex;
  border: 2rpx solid #93b4f8;
  border-radius: 36rpx;
  margin-left:32rpx;
  width:350rpx;
  padding:8rpx 0 0 6rpx;
  font-size:30rpx;
}
.search-icon{
  margin-left: 15rpx;
  margin-right: 5rpx;
  margin-top: 2rpx;
  width: 50rpx;
  height:40rpx;
}
/*租赁机构信息样式*/
.lease{
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  /* border: 1rpx #f2f2f2 solid; */
  width: 100%;
  height: 320rpx;
}
.lease-icon image{
  width: 400rpx;
  height: 225rpx;
  border-radius: 20rpx;
  margin: 40rpx 20rpx 0 20rpx;
}
.lease-icon{
  display: flex;
  flex-wrap: wrap;
  flex-direction: column;
}
.lease-content{
  display: flex;
  flex-wrap: wrap;
  flex-direction: column;
}
.lease-name{
  font-size: 36rpx;
  margin-top: 30rpx;
  font-weight: 500;
}
.lease-place{
  font-size: 32rpx;
  font-weight: 500;
  margin-top: 0rpx;
}
.lease-type{
  margin-top: 20rpx;
}
.lease-type .typec{
  color: #ffbc6b;
  font-size: 20rpx;
  border: 3.5rpx #ffbc6b solid;
  border-radius: 10rpx;
  margin: 0rpx 5rpx 0 5rpx;
}
.lease .grade{
  font-size: 48rpx;
  color: #ffbc6b;
  margin-top:25rpx;
}
.other{
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
}
.other-colum{
  display: flex;
  flex-wrap: wrap;
  flex-direction: column;
}
.other-colum .distance{
  font-size: 24rpx;
  color: #999999;
  margin-top:20rpx;
  margin-left: 30rpx; 
}
.other-colum .numbers{
  font-size: 24rpx;
  color: #999999;
  margin-top:10rpx;
  margin-left: 30rpx; 
}
.other image{
  width: 10rpx;
  height: 20rpx;
  margin-top: 75rpx;
  margin-left: 12rpx;
}



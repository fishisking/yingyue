/*头部标签*/
.User-content{
  display: flex;
}
.label{
  z-index: 1;
  margin-top: -432rpx;
  margin-left:610rpx;
}
/*头部背景图片*/
.header-pic{
  width: 100%;
  height: 420rpx;
  z-index: 1;
}
/*用户信息样式*/
.flex-row{
  flex-direction: row;
  display: flex;
}
.header-icon{
  display: flex;           
  justify-content: center;
  z-index: 1;   
}
.header-icon image{
  margin-top: 294rpx;
  width: 120rpx;
  height: 120rpx;
  border-radius: 80rpx; 
}
.user-info{
  display: flex;           
  justify-content: center;
  z-index: 1; 
}
.user-info .user-name{
  margin-top: 16rpx;
  font-size: 40rpx;
  color:#303030;
  font-weight: 500;
}
.phone{
  font-size: 26rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  align-content: center;
  width: 110rpx;
  color: #5594ff;
  border: 2rpx #5594ff solid;
  border-radius: 10rpx;
  margin-left: 40rpx;
}
.phone image{
  width: 26rpx;
  height: 26rpx;
}

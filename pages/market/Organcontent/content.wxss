.introduce {
  display: flex;           
  justify-content: center;
  margin-top: 15rpx; 
  }
.location{
  display: flex;
  flex-direction: row;
}
.location image{
  width: 40rpx;
  height: 55rpx;
}
.introduce .location-name{
  font-size: 34rpx;
  color: #8c8c8c;
  margin-left: 10rpx;
}

/*粉丝、反馈分、成交数、信用分*/
.details{
  flex-direction: row;
  display: flex;
  margin-top: 16rpx;
  align-content: center;
  justify-content: center;
}
.details .cut{
  width: 2rpx;
  height: 32rpx;
  background-color: #f2f2f2;
  margin-top: 44rpx;
}
.details .nums{
  font-size: 32rpx;
  color: #303030;
  text-align: center;
}

.details .name{
  font-size: 24rpx;
  color: #8c8c8c;
  text-align: center;
}
.fans{
  flex-direction: column;
  display: flex;
  width: 80rpx;
  margin-left: 45rpx;
  margin-right: 32rpx;
}
.feedback{
  flex-direction: column;
  display: flex;
  width: 80rpx;
  margin-left: 32rpx;
  margin-right: 31rpx;
} 
.deal{
  flex-direction: column;
  display: flex;
  width: 80rpx;
  margin-left: 32rpx;
  margin-right: 31rpx;
} 
.credit{
  flex-direction: column;
  display: flex;
  width: 80rpx;
  margin-left: 32rpx;
  margin-right: 31rpx;
} 

/*可选服务、活动、作品、评价*/
.onTabLine{
  width: 120rpx;
  height: 5rpx;
  background-color:#5594ff;
  margin-top:15rpx;
}
.selectTab{
  margin-top: 36rpx; 
}
.selectservice,.activity,.works,.review{
  display: flex;
  flex-direction: column;
  font-size: 36rpx;
  color:#aeaeae;
  width: 160rpx;
  /*添加下面句可以展现出动态效果*/
  /* justify-content: center; */
  align-content: center;
  align-items: center;
}
.activity{
  margin-left: 60rpx;
}
.review{
  margin-right: 40rpx;
}
.active{
  color:#5594ff;
}
/*首页活动*/
.index-images-display{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin: 20rpx 54rpx 20rpx 54rpx;
}
.index-show-list image{
  margin-top: 30rpx;
  width: 630rpx;
  height: 280rpx;
  box-shadow: 4rpx 6rpx 6rpx #CDCDCD;
  background: #aeaeae;
  border-radius: 20rpx;
  position: relative;
} 
/*套餐系列样式*/
.set-header{
  flex-direction: row;
  display: flex;
  margin-top: 40rpx;
}
.set-name{
  font-size: 32rpx;
  color: #303030;
  font-weight: 500;
  margin-left: 80rpx;
  width: 230rpx;
}
.set-price{
  font-size: 32rpx;
  color: #ffa942;
  font-weight: 500;
  width: 190rpx;
  margin-left: 70rpx;
}
.set-type{
  font-size: 28rpx;
  color: #aeaeae;
  font-weight: 500;
}
.set-type .typec{
  border:2rpx #aeaeae solid;
  border-radius: 10rpx;
}
/*套餐中图片的展示*/
.images-display{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin-top: 20rpx;
  margin-left: 60rpx;
  margin-right: 60rpx;
}
.show-list image{
  margin-top: 20rpx;
  width: 300rpx;
  height: 225rpx;
  box-shadow: 4rpx 6rpx 6rpx #CDCDCD;
  background: #aeaeae;
  border-radius: 20rpx;
  position: relative;
} 

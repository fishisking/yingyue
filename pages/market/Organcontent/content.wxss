/*评价*/
.valuation-content{
  display: flex;
  flex-direction: column;
  margin-top: 32rpx;
  margin-left: 68rpx;
}
.make-stars{
  display: flex;
  flex-direction: row;

}
.make-stars text{
  color: #ffd075;
  font-size: 40rpx;
}
.make-stars .stars{
  margin-left: 20rpx;
  
}
.make-stars .stars image{
  margin-top: 12rpx;
  margin-right: 10rpx;
  width: 36rpx;
  height: 36rpx;
}
.comment{
  margin-top: 20rpx;
  font-size: 28rpx;
  width: 600rpx;
}
.line{
  margin-top: 28rpx;
  width: 600rpx;
  height: 2rpx;
  background-color: #f2f2f2;
  /* margin-left: 45rpx; */
}
/*评论数*/
.comment-nums{
  margin-top: 20rpx;
  color:#CDCDCD;
  font-size: 24rpx;
  text-align: center;
}

/*底部导航*/
/*私信、关注、预约*/
.bottom-tab{
  bottom:0;
  height: 118rpx;
  width: 100%;
  background: linear-gradient(to right, #5594ff , #3bc3ff);
  position: fixed;
  z-index: 9999;
}
.bottom-tab.iphoneX-height{
    padding-bottom: 20rpx;
}
.tab-content{
  flex-direction: row;
  display: flex;
  justify-content:center;
  align-content: center;
}
.letter,.viewer,.book{
  margin-top: 30rpx;
  color: #ffffff;
  font-size: 36rpx;
  width:200rpx;
  text-align: center;
}


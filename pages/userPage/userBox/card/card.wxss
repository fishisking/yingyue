/* pages/userPage/userBox/card/card.wxss */

.cards{
  margin-top: 60rpx;
  display: flex;
  width: 100%;
  justify-content: center;
}

.item{
  margin-left: 30rpx;
  margin-right: 30rpx;
  box-shadow: 0 6rpx 24rpx rgba(25, 118, 210, 0.08);
  border-radius: 18rpx;
}


.itemTitle{
  display: flex;
  flex-direction: row;
  margin-left: 30rpx;
  margin-top: 30rpx;
  font-size: 35rpx;
  line-height: 50rpx;
}

.itemTitle image{
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  margin-right: 20rpx;
}

.itemTitle .name{
  line-height: 50rpx;
}

.content{
  display: flex;
  flex: row;

  margin-left: 30rpx;
  margin-right: 30rpx;
  height: 240rpx;
  width: 630rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-image: url("data:image/png;base64,")
}





.price{
  display: flex;
  flex-direction: column;
  margin-left: 150rpx;
  margin-top: 50rpx;
  width: 400rpx;
}

.yuan{
  font-size: 20rpx;
  line-height: 50rpx;
  color: #f14829
}

.number{
  font-size: 50rpx;
  color: #f14829
}

.clause{
  font-size: 20rpx;
  color: #545454;
  margin-bottom: 10rpx;
}

.useIt{
  font-size: 28rpx;
  margin-top: 45rpx;
  color: #f14829
}